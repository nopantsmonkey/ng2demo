{
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": ["es5", "dom"],
    "strictNullChecks": false,
    "types": [
      "core-js",
      "d3",
      "d3.cloud.layout",
      "google.visualization",
      "jasmine",
      "jquery",
      "lodash",
      "source-map",
      "uglify-js",
      "webpack",
      "node"
    ]
  },
  "files": [],
  "exclude": [
    "node_modules",
    "dist"
  ]
}


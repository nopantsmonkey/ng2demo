{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "lib": ["es5", "dom"],
    "strictNullChecks": false,
    "declaration": false,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
    "outDir": "./dist",
    "rootDir": "./app",
    "types": [
      "core-js",
      "d3.cloud.layout",
      "google.visualization",
      "jasmine",
      "jquery",
      "lodash",
      "source-map",
      "uglify-js",
      "webpack",
      "node"
    ]
  },
  "angularCompilerOptions": {
    "genDir": "./ngfactory"
  },
  "compileOnSave": false,
  "files": [
    "app/main.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
